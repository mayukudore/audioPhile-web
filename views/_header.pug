#cart.modal.fade(tabindex='-1' aria-labelledby='exampleModalLabel' aria-hidden='true')
  .modal-dialog.modal-dialog-centered.modal-dialog-scrollable.modal-lg
    .modal-content.modal-box
      
      .modal-body
        .container-fluid
          .row.cart-modal-header.justify-content-between
            .col-sm-6.col-9
              h4.cart-text CART 
                span (3)
            .col-sm-6.col-3.ml-auto.p-2
              a.remove-all(href='#') Remove all
          .row
            .col-sm-6.col-8.cart-images  
              img(src='/images/cart/image-xx99-mark-two-headphones.jpg')
              .cart-cost-container
                h6.product-name-cart XX99 MK II 
                h4.cost-of-product $2,999
            .col-sm-6.col-4.cart-quantity
              .increase-decrease-product
                button.remove-item -
                span.number-of-items 1
                button.add-item +
          .row
            .col-sm-6.col-8.cart-images 
              img(src='/images/cart/image-xx59-headphones.jpg')
              .cart-cost-container
                h6.product-name-cart XX59  
                h4.cost-of-product $899
            .col-sm-6.col-4.cart-quantity
              .increase-decrease-product 
                button.remove-item -
                span.number-of-items 2
                button.add-item +
          .row
            .col-sm-6.col-8.cart-images 
              img(src='/images/cart/image-yx1-earphones.jpg')
              .cart-cost-container
                h6.product-name-cart YX1 
                h4.cost-of-product $599
            .col-sm-6.col-4.cart-quantity
              .increase-decrease-product 
                button.remove-item -
                span.number-of-items 1
                button.add-item +
          .row
            .col-sm-6.col-9.total-amount TOTAL

            .col-sm-6.col-3 $5,396



      .modal-footer.footer.box
        .row
          .col-sm-6.col-10
            button.see-product-button.checkout-button-cart(type='button' data-bs-dismiss='modal') CHECKOUT
